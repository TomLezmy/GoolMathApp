<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners
        android:radius="10dp" />
    <gradient android:angle="45"
        android:startColor="@color/colorBtn4"
        android:endColor="@color/colorBtn4"/>
    <padding
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp" />
    <size
        android:width="125dp"
        android:height="125dp" />

    <stroke
        android:width="3dp"
        android:color="@color/colorStrokeBtn4"/>

</shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners
                android:radius="10dp" />
            <gradient android:angle="45"
                android:startColor="@color/colorStrokeBtn4"
                android:endColor="@color/colorStrokeBtn4"/>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
            <size
                android:width="125dp"
                android:height="125dp" />

            <stroke
                android:width="3dp"
                android:color="@color/colorBtn4"/>

        </shape>
    </item>
</selector>